  <data name="customSqlQuery1.Sql" xml:space="preserve">
    <value>SELECT  
    P.ID_PROYECTO AS ID_PROYECTO_PARAM,
    P.NAME_PROYECTO AS PROYECTO,
    U.USER_NAME AS SOLICITANTE,
    UA.USER_NAME AS AUTORIZADO,
    M.TEXT_ID AS MUESTRA,
    M.CHANGED_ON AS FECHA,
    M.OBSERVACIONES AS OBSERVACIONES,
    PL.NAME_PLANTA AS LOTE,
    C.NAME_CLIENTE AS CLIENTE,
    M.RECD_DATE AS 'FECHA RECEPCIÓN',
    A.ALIAS_NAME AS ANALISIS,
    CASE
        WHEN R.RESULT_NUMBER &lt; TRY_CAST(SUBSTRING(AD.CLAMP_LOW, 2, LEN(AD.CLAMP_LOW)-1) AS FLOAT) THEN AD.CLAMP_LOW
        WHEN R.RESULT_NUMBER &gt; TRY_CAST(SUBSTRING(AD.CLAMP_HIGH, 2, LEN(AD.CLAMP_HIGH)-1) AS FLOAT) THEN AD.CLAMP_HIGH
        ELSE CAST(CAST(R.RESULT_NUMBER AS FLOAT) AS VARCHAR(20))
    END AS VALOR
FROM 
    PROYECTO P
    INNER JOIN MUESTRA M ON P.ID_PROYECTO = M.PROJECT
    INNER JOIN PLANTA PL ON M.ID_PLANTA = PL.ID_PLANTA
    INNER JOIN CLIENTE C ON C.ID_CLIENTE = M.ID_CLIENTE
    INNER JOIN USUARIO U ON M.LOGIN_BY = U.ID_USER
    INNER JOIN USUARIO UA ON M.REVIEWER = UA.ID_USER
    INNER JOIN RESULTADO R ON M.ID_SAMPLE = R.ID_SAMPLE
    INNER JOIN ANALISIS_DETALLE AD ON R.ID_ANALYSIS = AD.ID_ANALISIS AND R.ID_COMPONENT = AD.ID_COMPONENT
    INNER JOIN ANALISIS A ON AD.ID_ANALISIS = A.ID_ANALISIS
WHERE 
    AD.REPORTABLE = 1
    AND M.STATUS = 21</value>
  </data>
